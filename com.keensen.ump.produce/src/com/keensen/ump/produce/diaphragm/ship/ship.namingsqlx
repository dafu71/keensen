	</isNotNull>
     <isNotNull prepend = "and"  property = "planDateStart"> 
          to_char(t2.plan_date,'yyyy-MM-dd')  &gt;=  #planDateStart#
     </isNotNull>
     <isNotNull prepend = "and"  property = "planDateEnd"> 
          to_char(t2.plan_date,'yyyy-MM-dd')  &lt;=  #planDateEnd#
     </isNotNull>
	order by T.ORDER_NO,t2.PLAN_NO
     </select>
     
