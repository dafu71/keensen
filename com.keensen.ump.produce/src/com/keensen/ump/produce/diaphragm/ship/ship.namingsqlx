<sqlMap>
    <select id="queryTumo" resultClass="commonj.sdo.DataObject" parameterClass="java.util.HashMap" >
     select t1.RECORD_ID       as "recordId",t1.is_wx "isWx",t1.worker_id "workerId"
       ,t1.spec_id "spectId",t1.line_id "lineId",t1.is_cut_over "isCutOver",
       t1.LINE_NAME       as "lineName",
       t1.LINE_CODE       as "lineCode",
       t1.PRODUCE_DT      as "produceDt",
       t1.BATCH_NO        as "batchNo",
       lower(t1.IS_QUALIFIED)    as "isQualified",
       t1.OUT_LENGTH      as "outLength",
       t1.LOSS            as "loss",
       t1.USEFUL_LENGTH as "usefulLength",t1.QUALIFID_LENGTH as "qualifidLength",
       t1.PLAN_NO         as "planNo",
       t1.ORDER_NO        as "orderNo",
       t1.SUP_ID          as "supId",
       t1.SUP_NAME        as "supName",
       t1.SUP_CODE        as "supCode",
       t1.DIMO_BATCH_NO   as "dimoBatchNo",
       t1.OUT_BATCH_NO    as "outBatchNo",
       t1.DEPT_NAME       as "deptName",
       t1.STAFF_NAME      as "staffName",
       t1.STAFF_CODE      as "staffCode",
       t1.PERF_FLAG_ID    as "perfFlagId",
       t1.PERF_FLAG_NAME  as "perfFlagName",
       t1.PERF_FLAG_CODE  as "perfFlagCode",
       <!--decode(t2.RECORD_ID,null,'n','y') "shipflag",
       nvl(t2.ifconfirm,'n')    "ifconfirm",-->
       t1.mater_class_code as "materClassCode",
       t1.mater_spec_code as "materSpecCode",
       t1.mater_class_Name as "materClassName",
       t1.mater_spec_Name as "materSpecName",
       ddflag "ddflag",
       <!--to_char(t2.create_time,'yyyy-MM-dd') "createTime",
       t2.create_name "createName", 
       t2.amount "sendAmount",
       t2.id "sendId",-->
       t1.QUALIFID_LENGTH - nvl(t3.amount,0) "delivery"
  from v_ks_tumo3 t1
  
  left join (SELECT sum(nvl(t.amount,0)) amount ,record_id 
			from KS_PROD_DIAPHRAGM_SHIP t
			GROUP BY record_id) t3
	on t1.RECORD_ID = t3.RECORD_ID
 
 where 1=1
    <isEqual prepend="and" property="isDelivery" compareValue="N">
 			   t1.QUALIFID_LENGTH - nvl(t3.amount,0) > 0
    </isEqual>
 	<isNotNull prepend="and" property="isCutOver"> 
		       upper(nvl(t1.is_cut_over,'N')) = 'N'
	</isNotNull>
 	<isNotNull prepend="and" property="isQualified"> 
		       upper(t1.IS_QUALIFIED) = upper(#isQualified#)
	</isNotNull>
    <isNotNull prepend="and" property="ifconfirm"> 
		       nvl(t2.ifconfirm,'n') = #ifconfirm#
	</isNotNull>
    <isNotNull prepend="and" property="ddflag"> 
		       t1.ddflag = #ddflag#
	</isNotNull>
    <isNotNull prepend="and" property="perfFlagId"> 
		       t1.PERF_FLAG_ID = #perfFlagId#
	</isNotNull>
 	<isNotNull prepend="and" property="shipflag"> 
		       decode(t2.RECORD_ID,null,'n','y') = #shipflag#
	</isNotNull>
    <isNotNull prepend="and" property="yrkflag"> 
		       decode(w.batch_no,null,'否','是') = #yrkflag#
	</isNotNull>
 	<isNotNull prepend="and" property="lineId"> 
		       t1.line_id = #lineId#
	</isNotNull> 
	<isNotNull prepend="and" property="specId"> 
		       t1.spec_id = #specId#
	</isNotNull>
	<isNotNull prepend="and" property="workerId"> 
		       t1.worker_Id = #workerId#
	</isNotNull>
	<isNotNull prepend="and" property="supId"> 
		       t1.SUP_ID = #supId#
	</isNotNull>
	<isNotNull prepend="and" property="isWx"> 
		       t1.is_wx = #isWx#
	</isNotNull>		
    <isNotNull prepend="and" property="recordId"> 
		       t1.RECORD_ID = #recordId#
	</isNotNull>
 
	<isNotNull prepend="and" property="batchNo"> 
		       t1.batch_No = #batchNo#
	</isNotNull>
	
	<isNotNull prepend="and" property="batchNoStr"> 
		       t1.batch_No in ($batchNoStr$)
	</isNotNull>
	
	<isNotNull prepend="and" property="batchNo2"> 
		       t1.batch_No like '%' || upper(#batchNo2#) || '%'
	</isNotNull>
	
	<isNotNull prepend = "and"  property = "produceDtStart"> 
          to_char(t1.PRODUCE_DT,'yyyy-MM-dd hh24:mi:ss')  &gt;=  #produceDtStart#
     </isNotNull>
     <isNotNull prepend = "and"  property = "produceDtEnd"> 
          to_char(t1.PRODUCE_DT,'yyyy-MM-dd hh24:mi:ss')  &lt;=  #produceDtEnd#
     </isNotNull>
     <isNotNull prepend="and" property="orderNo"> 
		       upper(t1.order_No) like '%' || upper(#orderNo#) || '%'
	</isNotNull>
	<isNotNull prepend="and" property="planNo"> 
		       upper(t1.plan_No) like '%' || upper(#planNo#) || '%'
	</isNotNull>
	<isNotNull prepend="and" property="dimoBatchNo"> 
		       t1.DIMO_BATCH_NO like '%' || #dimoBatchNo# || '%'
	</isNotNull>
 order by t1.produce_dt desc
 </select>
 
 <select id="queryTumoSum" resultClass="commonj.sdo.DataObject" parameterClass="java.util.HashMap" >
  SELECT SUM(T1.OUT_LENGTH) AS "outLength",
       SUM(T1.LOSS) AS "loss",
       SUM(T1.USEFUL_LENGTH) AS "usefulLength",
       SUM(T1.QUALIFID_LENGTH) AS "qualifidLength",
       SUM(DECODE(T1.PERF_FLAG_CODE, 'A', T1.QUALIFID_LENGTH, 0)) AS "aqualifidLength",
       SUM(DECODE(T1.PERF_FLAG_CODE, 'B', T1.QUALIFID_LENGTH, 0)) AS "bqualifidLength",
       SUM(DECODE(T1.PERF_FLAG_CODE, 'C', T1.QUALIFID_LENGTH, 0)) AS "cqualifidLength"
  FROM V_KS_TUMO3 T1
  LEFT JOIN KS_PROD_DIAPHRAGM_SHIP T2
    ON T1.RECORD_ID = T2.RECORD_ID
 WHERE 1 = 1
    <isNotNull prepend="and" property="isCutOver"> 
		       upper(nvl(t1.is_cut_over,'N')) = 'N'
	</isNotNull>
    <isNotNull prepend="and" property="ifconfirm"> 
		       nvl(t2.ifconfirm,'n') = #ifconfirm#
	</isNotNull>
    <isNotNull prepend="and" property="ddflag"> 
		       t1.ddflag = #ddflag#
	</isNotNull>
    <isNotNull prepend="and" property="perfFlagId"> 
		       t1.PERF_FLAG_ID = #perfFlagId#
	</isNotNull>
 	<isNotNull prepend="and" property="shipflag"> 
		       decode(t2.RECORD_ID,null,'n','y') = #shipflag#
	</isNotNull>
    <isNotNull prepend="and" property="yrkflag"> 
		       decode(w.batch_no,null,'否','是') = #yrkflag#
	</isNotNull>
 	<isNotNull prepend="and" property="lineId"> 
		       t1.line_id = #lineId#
	</isNotNull> 
	<isNotNull prepend="and" property="specId"> 
		       t1.spec_id = #specId#
	</isNotNull>
	<isNotNull prepend="and" property="workerId"> 
		       t1.worker_Id = #workerId#
	</isNotNull>
	<isNotNull prepend="and" property="supId"> 
		       t1.SUP_ID = #supId#
	</isNotNull>
	<isNotNull prepend="and" property="isWx"> 
		       t1.is_wx = #isWx#
	</isNotNull>		
    <isNotNull prepend="and" property="recordId"> 
		       t1.RECORD_ID = #recordId#
	</isNotNull>
 
	<isNotNull prepend="and" property="batchNo"> 
		       t1.batch_No = #batchNo#
	</isNotNull>
	
	<isNotNull prepend="and" property="batchNo2"> 
		       t1.batch_No like '%' || upper(#batchNo2#) || '%'
	</isNotNull>
	
	<isNotNull prepend = "and"  property = "produceDtStart"> 
          to_char(t1.PRODUCE_DT,'yyyy-MM-dd hh24:mi:ss')  &gt;=  #produceDtStart#
     </isNotNull>
     <isNotNull prepend = "and"  property = "produceDtEnd"> 
          to_char(t1.PRODUCE_DT,'yyyy-MM-dd hh24:mi:ss')  &lt;=  #produceDtEnd#
     </isNotNull>
     <isNotNull prepend="and" property="orderNo"> 
		       upper(t1.order_No) like '%' || upper(#orderNo#) || '%'
	</isNotNull>
	<isNotNull prepend="and" property="planNo"> 
		       upper(t1.plan_No) like '%' || upper(#planNo#) || '%'
	</isNotNull>
	<isNotNull prepend="and" property="dimoBatchNo"> 
		       t1.DIMO_BATCH_NO like '%' || #dimoBatchNo# || '%'
	</isNotNull>

 </select>
 
 <select id="queryFhd" resultClass="commonj.sdo.DataObject" parameterClass="java.util.HashMap" >
     select t1.RECORD_ID       as "recordId", t1.is_wx "isWx",t1.worker_id "workerId",t2.create_name "createName",
       t1.LINE_NAME       as "lineName",t1.line_id "lineId",t2.id "id", t2.amount "sendAmount",
       to_char(t2.create_time,'yyyy-MM-dd') "createTime",
       nvl(t2.ifconfirm,'n')    "ifconfirm",
       t1.LINE_CODE       as "lineCode",
       t1.PRODUCE_DT      as "produceDt",
       t1.BATCH_NO        as "batchNo",
       t1.IS_QUALIFIED    as "isQualified",
       t1.OUT_LENGTH      as "outLength",
       t1.LOSS            as "loss",
       t1.USEFUL_LENGTH as "usefulLength",t1.QUALIFID_LENGTH as "qualifidLength",
       t2.PLAN_NO         as "planNo",
       t2.ORDER_NO        as "orderNo",
       t2.delivery_order_no as "deliveryOrderNo",
        
       to_char(t2.delivery_dt,'YYYY-MM-DD HH24:MI:SS') as "deliveryDt",
       
       t1.SUP_ID          as "supId",
       t1.SUP_NAME        as "supName",
       t1.SUP_CODE        as "supCode",
       t1.DIMO_BATCH_NO   as "dimoBatchNo",
       t1.OUT_BATCH_NO    as "outBatchNo",
       t1.DEPT_NAME       as "deptName",
       t1.STAFF_NAME      as "staffName",
       t1.STAFF_CODE      as "staffCode",
       t1.PERF_FLAG_ID    as "perfFlagId",
       t1.PERF_FLAG_NAME  as "perfFlagName",
       t1.PERF_FLAG_CODE  as "perfFlagCode",
       decode(t2.RECORD_ID,null,0,1) "rkflag",
       t1.mater_class_code as "materClassCode",
       t1.mater_spec_code as "materSpecCode",
       t1.mater_class_Name as "materClassName",
       t1.mater_spec_Name as "materSpecName"
  from V_KS_PROD_DIAPHRAGM_SHIP t2
  left join V_KS_TUMO3 t1 
    on t1.RECORD_ID = t2.RECORD_ID
 where 1=1
    <isNotNull prepend="and" property="ifconfirm"> 
		       nvl(t2.ifconfirm,'n') = #ifconfirm#
	</isNotNull>
   <isNotNull prepend="and" property="perfFlagId"> 
		       t1.PERF_FLAG_ID = #perfFlagId#
	</isNotNull>
 
   <isNotNull prepend="and" property="lineId"> 
		       t1.line_id = #lineId#
	</isNotNull> 
	<isNotNull prepend="and" property="specId"> 
		       t1.spec_id = #specId#
	</isNotNull>
	<isNotNull prepend="and" property="workerId"> 
		       t1.worker_Id = #workerId#
	</isNotNull>
	<isNotNull prepend="and" property="supId"> 
		       t1.SUP_ID = #supId#
	</isNotNull>
	<isNotNull prepend="and" property="isWx"> 
		       t1.is_wx = #isWx#
	</isNotNull>		
    <isNotNull prepend="and" property="recordId"> 
		       t1.RECORD_ID = #recordId#
	</isNotNull>

	<isNotNull prepend="and" property="batchNo"> 
		       t1.batch_No = #batchNo#
	</isNotNull>
	
	
	<isNotNull prepend="and" property="batchNo2"> 
		       t1.batch_No like '%' || upper(#batchNo2#) || '%'
	</isNotNull>
	
	<isNotNull prepend = "and"  property = "produceDtStart"> 
          to_char(t1.PRODUCE_DT,'yyyy-MM-dd')  &gt;=  #produceDtStart#
     </isNotNull>
     <isNotNull prepend = "and"  property = "produceDtEnd"> 
          to_char(t1.PRODUCE_DT,'yyyy-MM-dd')  &lt;=  #produceDtEnd#
     </isNotNull>
     <isNotNull prepend="and" property="orderNo"> 
		       upper(t1.order_No) like '%' || upper(#orderNo#) || '%'
	</isNotNull>
	<isNotNull prepend="and" property="planNo"> 
		       upper(t1.plan_No)  like '%' || upper(#planNo#) || '%'
	</isNotNull>
	<isNotNull prepend="and" property="dimoBatchNo"> 
		       t1.DIMO_BATCH_NO  like '%' || #dimoBatchNo# || '%'
	</isNotNull>
	<isNotNull prepend="and" property="orderNo2"> 
		       upper(t1.order_No) = upper(#orderNo2#) 
	</isNotNull>
	
	<isNotNull prepend="and" property="deliveryOrderNo"> 
		       t2.delivery_Order_No like '%' || #deliveryOrderNo# || '%'
	</isNotNull>
	
	
	<isNotNull prepend = "and"  property = "deliveryDtStart"> 
          to_char(t2.delivery_DT,'yyyy-MM-dd')  &gt;=  #deliveryDtStart#
     </isNotNull>
     <isNotNull prepend = "and"  property = "deliveryDtEnd"> 
          to_char(t2.delivery_DT,'yyyy-MM-dd')  &lt;=  #deliveryDtEnd#
     </isNotNull>
     
     <isNotNull prepend="and" property="batchNoStr"> 
		       t1.batch_No in ($batchNoStr$)
	</isNotNull>
	
 order by t2.delivery_Order_No desc nulls last, t1.produce_dt desc 
     </select>
 
 	<select id="queryOrder" resultClass="commonj.sdo.DataObject" parameterClass="java.util.HashMap" >
      
	     SELECT T1.ID                "id",t1.ORDER_DATE "orderDate",t1.create_name "createName",
	       T1.ORDER_NO          "orderNo", t1.customer_Code "customerCode",
	       T1.AMOUNT            "amount",
           T1.SPEC_ID           "specId",T1.remark "remark",
           t1.unit as "unit",
           t1.demand_date as "demandDate",
			t1.ext0 as "ext0",
			t1.ext1 as "ext1",
			t1.ext2 as "ext2",
			t1.ext3 as "ext3",
			t1.ext4 as "ext4",
			t1.ext5 as "ext5",
			t1.ext6 as "ext6",
			t1.ext7 as "ext7",
			t1.ext8 as "ext8",
			t1.ext9 as "ext9",
			t1.ext10 as "ext10",
			t1.ext11 as "ext11",
			t1.ext12 as "ext12",
			t1.ext13 as "ext13",
			t1.ext14 as "ext14",
			t1.ext15 as "ext15",
			t1.ext16 as "ext16",
			t1.ext17 as "ext17",
			t1.ext18 as "ext18",
			t1.ext19 as "ext19",
           t1.need_amount "needAmount",
           t1.arrange_percent "arrangePercent",
           round(T1.NEED_AMOUNT / (NVL(T1.ARRANGE_PERCENT, 100) / 100),1) "arrangeAmount",
           t1.order_status "orderStatus",
	       T99.MATER_CLASS_CODE AS "materClassCode",
	       T22.MATER_SPEC_CODE  AS "materSpecCode",
	       T99.MATER_CLASS_NAME AS "materClassName",
	       T22.MATER_SPEC_NAME  AS "materSpecName",
	       nvl(wp.cnt,0) "planCount"
	  FROM KS_PROD_DIAPHRAGM_ORDER T1
	  left join (select t.relation_id,COUNT(1) cnt from KS_PROD_DIAPHRAGM_WORKPLAN t
		GROUP BY t.relation_id) wp on wp.relation_id = t1.id
	  LEFT JOIN QINSEN.PARA_MATER_SPEC T22
	    ON T1.SPEC_ID = T22.MATER_SPEC_ID
	  LEFT JOIN QINSEN.PARA_MATER_CLASS T99
	    ON T99.MATER_CLASS_ID = T22.MATER_CLASS_ID
     where 1=1
     <isEqual prepend="and" property="isClosed" compareValue="Y">
 			   t1.order_status = '进行'
    </isEqual>
     <isNotNull prepend = "and"  property = "orderDateStart"> 
          to_char(t1.ORDER_DATE,'yyyy-MM-dd')  &gt;=  #orderDateStart#
     </isNotNull>
     <isNotNull prepend = "and"  property = "orderDateEnd"> 
          to_char(t1.ORDER_DATE,'yyyy-MM-dd')  &lt;=  #orderDateEnd#
     </isNotNull>
      <isNotNull prepend = "and"  property = "demandDateStart"> 
          to_char(t1.demand_DATE,'yyyy-MM-dd')  &gt;=  #demandDateStart#
     </isNotNull>
     <isNotNull prepend = "and"  property = "demandDateEnd"> 
          to_char(t1.demand_DATE,'yyyy-MM-dd')  &lt;=  #demandDateEnd#
     </isNotNull>
     <isNotNull prepend="and" property="specId"> 
		       t1.spec_id = #specId#
	</isNotNull>
	<isNotNull prepend="and" property="id"> 
		       T1.ID = #id#
	</isNotNull>
	<isNotNull prepend="and" property="orderNo"> 
		       upper(t1.order_No) like '%' || upper(#orderNo#) || '%'
	</isNotNull>
	<isNotNull prepend="and" property="orderNo2"> 
		       t1.order_No = #orderNo2# 
	</isNotNull>
	<isNotNull prepend="and" property="planNo"> 
		       t1.order_No in( select order_No from KS_PROD_DIAPHRAGM_PLAN where plan_no like '%' || #planNo# || '%')
	</isNotNull>
	order by t1.id desc
     </select>
     
     <select id="queryPlan" resultClass="commonj.sdo.DataObject" parameterClass="java.util.HashMap" >
      
	     SELECT T1.ID                "id", t1.create_name "createName",
	       T1.ORDER_NO          "orderNo",
	       T1.PLAN_NO          "planNo",
	       T1.plan_date "planDate",
	       T1.AMOUNT            "amount",
	       T1.plan_stock_date "planStockDate",
	       T1.stock_AMOUNT            "stockAmount",
	       T2.amount            "orderAmount",t2.ORDER_DATE "orderDate",
           T1.SPEC_ID           "specId",T1.remark "remark",
	       T99.MATER_CLASS_CODE AS "materClassCode",
	       T22.MATER_SPEC_CODE  AS "materSpecCode",
	       T99.MATER_CLASS_NAME AS "materClassName",
	       T22.MATER_SPEC_NAME  AS "materSpecName"
	  FROM KS_PROD_DIAPHRAGM_PLAN T1
	  LEFT JOIN KS_PROD_DIAPHRAGM_ORDER T2
	    ON T1.ORDER_NO = T2.ORDER_NO
	  LEFT JOIN QINSEN.PARA_MATER_SPEC T22
	    ON T1.SPEC_ID = T22.MATER_SPEC_ID
	  LEFT JOIN QINSEN.PARA_MATER_CLASS T99
	    ON T99.MATER_CLASS_ID = T22.MATER_CLASS_ID
     where 1=1
     <isNotNull prepend="and" property="specId"> 
		       t1.spec_id = #specId#
	</isNotNull>
	<isNotNull prepend="and" property="id"> 
		       T1.ID = #id#
	</isNotNull>
	<isNotNull prepend="and" property="orderNo"> 
		       upper(t1.order_No) like '%' || upper(#orderNo#) || '%'
	</isNotNull>
	<isNotNull prepend="and" property="planNo"> 
		       upper(t1.PLAN_NO) like '%' || upper(#planNo#) || '%'
	</isNotNull>
	<isNotNull prepend="and" property="planNo2"> 
		       t1.PLAN_NO = #planNo2# 
	</isNotNull>
	
	<isNotNull prepend = "and"  property = "planDateStart"> 
          to_char(t1.plan_date,'yyyy-MM-dd')  &gt;=  #planDateStart#
     </isNotNull>
     <isNotNull prepend = "and"  property = "planDateEnd"> 
          to_char(t1.plan_date,'yyyy-MM-dd')  &lt;=  #planDateEnd#
     </isNotNull>
     
     <isNotNull prepend = "and"  property = "planStockDateStart"> 
          to_char(t1.plan_stock_date,'yyyy-MM-dd')  &gt;=  #planStockDateStart#
     </isNotNull>
     <isNotNull prepend = "and"  property = "planStockDateEnd"> 
          to_char(t1.plan_stock_date,'yyyy-MM-dd')  &lt;=  #planStockDateEnd#
     </isNotNull>
     
	order by T2.ORDER_NO,t1.PLAN_NO
     </select>
     
     <select id="queryOrderTrace" resultClass="commonj.sdo.DataObject" parameterClass="java.util.HashMap" >
	     SELECT T.ORDER_NO           "orderNo",
         T.SPEC_ID            "specId",
         T99.MATER_CLASS_CODE AS "materClassCode",
         T22.MATER_SPEC_CODE  AS "materSpecCode",
         T99.MATER_CLASS_NAME AS "materClassName",
         T22.MATER_SPEC_NAME  AS "materSpecName",
         T.ORDER_DATE         "orderDate",
         T.AMOUNT             "orderAmount",
         T2.PLAN_NO           "planNo",
         T2.PLAN_DATE         "planDate",
         T2.AMOUNT            "planAmount",T2.remark "planRemark",
         TAMOUNT              "tamount",
         camount   "camount",
         aamount   "aamount",
         to_char(aamount/tamount * 100, 'FM990.00') || '%'    "passrate",
         sendamount "sendamount",t7.useful_amount "usefulAmount"
         ,t7.once_amount "onceAmount",t7.twice_amount "twiceAmount",t7.zero_amount "zeroAmount",t8.amount "stockAmount"
    FROM KS_PROD_DIAPHRAGM_ORDER T
    LEFT JOIN QINSEN.PARA_MATER_SPEC T22
      ON T.SPEC_ID = T22.MATER_SPEC_ID
    LEFT JOIN QINSEN.PARA_MATER_CLASS T99
      ON T99.MATER_CLASS_ID = T22.MATER_CLASS_ID
    LEFT JOIN KS_PROD_DIAPHRAGM_PLAN T2
      ON T.ORDER_NO = T2.ORDER_NO
    LEFT JOIN (SELECT V.ORDER_NO, V.PLAN_NO, SUM(V.OUT_LENGTH) TAMOUNT
                 FROM v_ks_tumo_all3 V
                WHERE V.ORDER_NO IS NOT NULL
                  AND V.PLAN_NO IS NOT NULL
                  <isNotNull prepend="and" property="isCutOver"> 
		       	upper(nvl(v.is_cut_over,'N')) = 'N'
				</isNotNull>
                GROUP BY V.ORDER_NO, V.PLAN_NO) T3
      ON T.ORDER_NO = T3.ORDER_NO
     AND T2.PLAN_NO = T3.PLAN_NO
     LEFT JOIN (SELECT V.ORDER_NO, V.PLAN_NO, SUM(V.QUALIFID_LENGTH) CAMOUNT
                 FROM v_ks_tumo_all3 V
                WHERE V.ORDER_NO IS NOT NULL
                  AND V.PLAN_NO IS NOT NULL
                   <isNotNull prepend="and" property="isCutOver"> 
		       	upper(nvl(v.is_cut_over,'N')) = 'N'
				</isNotNull>
                GROUP BY V.ORDER_NO, V.PLAN_NO) T4
      ON T.ORDER_NO = T4.ORDER_NO
     AND T2.PLAN_NO = T4.PLAN_NO
     LEFT JOIN (SELECT V.ORDER_NO, V.PLAN_NO, SUM(V.QUALIFID_LENGTH) AAMOUNT
                 FROM v_ks_tumo_all3 V
                WHERE V.ORDER_NO IS NOT NULL
                  AND V.PLAN_NO IS NOT NULL
                  AND v.perf_flag_code = 'A' <isNotNull prepend="and" property="isCutOver"> 
		       	upper(nvl(v.is_cut_over,'N')) = 'N'
				</isNotNull>
                  
                GROUP BY V.ORDER_NO, V.PLAN_NO) T5
      ON T.ORDER_NO = T5.ORDER_NO
     AND T2.PLAN_NO = T5.PLAN_NO
     left join (SELECT SUM(amount) sendamount,plan_no,order_no
    FROM
    (select t.batch_no,t.plan_no,t.order_no,t.amount from V_KS_PROD_DIAPHRAGM_SHIP t)
    GROUP BY plan_no,order_no) t6
     ON T.ORDER_NO = T6.ORDER_NO
     AND T2.PLAN_NO = T6.PLAN_NO
     
     LEFT JOIN (SELECT V.ORDER_NO, V.PLAN_NO, SUM(V.useful_LENGTH) useful_AMOUNT,
          sum(DECODE(v.JUDGEFLAG,1,V.useful_LENGTH,0)) once_amount,
          sum(DECODE(v.JUDGEFLAG,2,V.useful_LENGTH,0)) twice_amount,
          sum(DECODE(v.JUDGEFLAG,0,V.useful_LENGTH,0)) zero_amount 
                 FROM v_ks_tumo_all3 V
                WHERE V.ORDER_NO IS NOT NULL
                  AND V.PLAN_NO IS NOT NULL
                 <isNotNull prepend="and" property="isCutOver"> 
		       	upper(nvl(v.is_cut_over,'N')) = 'N'
				</isNotNull>
                GROUP BY V.ORDER_NO, V.PLAN_NO) T7
      ON T.ORDER_NO = T7.ORDER_NO
     AND T2.PLAN_NO = T7.PLAN_NO
     left join V_KS_diaphragm_stock_count t8
      ON T.ORDER_NO = T8.ORDER_NO
     AND T2.PLAN_NO = T8.PLAN_NO
     where 1=1
     <isNotNull prepend="and" property="orderNo"> 
		       upper(t.order_No) like '%' || upper(#orderNo#) || '%'
	</isNotNull>
	<isNotNull prepend="and" property="planNo"> 
